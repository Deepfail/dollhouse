# OpenRouter API Configuration
# Get your API key from https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# Default AI model to use
VITE_DEFAULT_MODEL=deepseek/deepseek-chat-v3.1

# Image generation settings (optional)
VITE_IMAGE_PROVIDER=none
VITE_IMAGE_API_KEY=your_venice_api_key_here_if_using_images

# Figma API Configuration
# Get your personal access token from https://www.figma.com/developers/api#access-tokens
FIGMA_ACCESS_TOKEN=your_figma_access_token_here